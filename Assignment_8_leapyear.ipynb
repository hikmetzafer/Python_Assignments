   "source": [
    "year = input('Write the year to check if it is leap year!\\n')\n",
    "\n",
    "leap_check = not (int(year) % 400) or not (int(year) % 4) and (int(year) % 100)\n",
    "\n",
    "print(f\"{year} is a leap year\") if leap_check else print(f\"{year} is not a leap year\")"
   ]
